{
	"name": "@hocuspocus/extension-redis",
	"version": "4.0.0-rc.0",
	"description": "Scale Hocuspocus horizontally with Redis",
	"homepage": "https://hocuspocus.dev",
	"keywords": [
		"hocuspocus",
		"redis",
		"yjs"
	],
	"license": "MIT",
	"type": "module",
