		"kleur": "^4.1.4",
		"lodash.debounce": "^4.0.8"
	},
	"peerDependencies": {
		"y-protocols": "^1.0.6",
		"yjs": "^13.6.8"
	},
	"gitHead": "b3454a4ca289a84ddfb7fa5607a2d4b8d5c37e9d",
